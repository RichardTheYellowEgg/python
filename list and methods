#practice 2
y=["a","b","c","d"]
y.remove("d")
print(y)
y.pop(2)
print(y)
y.clear()
# print(y)
# del y
# print(y)
#loop list:
z=[1,2,3,4]
for x in z:
    print(x)
for i in range(len(z)):
    print(z[i])
#while loop:
i=0
while i<len(z):
    print(z[i])
    i=i+1
#short hand loop:
[print(z)for x in y]































#practice 3
a=["a","b","c"]
for x in a:
    print(x)
for i in range(len(a)):
    print(a[i])
[print(a)for x in a]
i=0
while i<len(a):
    print(a[i])
    i=i+1
