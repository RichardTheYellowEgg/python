#practice
x=[1,2,3]
x[1]="2"
print(x)
x=[1,2,3,4,32,2]
x[4:6]=[5,6]
print(x)
x=[1,2,3,4,6,7]
x.insert(4,5)
print(x)
x=[1,2,3,4,5,6,7,8]
y=[9,10]
x.extend(y)
print(x)
x=[1,2,3,4,5]
x.append("e")
print(x)
#remove items:
#remove():
x=[1,31,2,3,4,5,6]
x.remove(31)
print(x)
#pop():
x.pop(5)
print(x)
#clear():
x.clear()
print(x)
